<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F2F8"
    android:orientation="vertical"
    tools:context=".DataExtracted">


    <View
        android:id="@+id/viewTop"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#FFF"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/image_inwi"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/inwi"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toStartOf="@+id/guideline7"
        app:layout_constraintStart_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/account_creation_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Résultats du Scan"
        android:textSize="20dp"
        android:layout_marginStart="6dp"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toBottomOf="@+id/viewTop" />


    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/gray_line"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline4" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_cin_result"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/border_identity_white"
        android:elevation="10dp"
        android:padding="6dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline9"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toBottomOf="@+id/account_creation_txt"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.4" />


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.92" />

        <TextView
            android:id="@+id/prenom_cin_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Prénom"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/nom_cin_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Nom"
            app:layout_constraintTop_toBottomOf="@+id/prenom_cin_label" />

        <TextView
            android:id="@+id/date_cin_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Date de naissance"
            app:layout_constraintTop_toBottomOf="@+id/nom_cin_label" />

        <TextView
            android:id="@+id/number_cin_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="CIN "
            app:layout_constraintTop_toBottomOf="@+id/date_cin_label" />

        <TextView
            android:id="@+id/adresse_cin_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Adresse "
            app:layout_constraintTop_toBottomOf="@+id/number_cin_label" />

        <TextView
            android:id="@+id/sexe_cin_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Sexe "
            app:layout_constraintTop_toBottomOf="@+id/adresse_cin_result" />

        <TextView
            android:id="@+id/prenom_cin_result"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="6dp"
            android:text="XXX "
            app:layout_constraintEnd_toStartOf="@+id/guideline10"
            app:layout_constraintStart_toStartOf="@+id/guideline11"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/nom_cin_result"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="6dp"
            android:text="-"
            app:layout_constraintEnd_toStartOf="@+id/guideline10"
            app:layout_constraintStart_toStartOf="@+id/guideline11"
            app:layout_constraintTop_toBottomOf="@+id/prenom_cin_result" />

        <TextView
            android:id="@+id/date_cin_result"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="6dp"
            android:text="YXYXYX  YXYX"
            app:layout_constraintEnd_toStartOf="@+id/guideline10"
            app:layout_constraintStart_toStartOf="@+id/guideline11"
            app:layout_constraintTop_toBottomOf="@+id/nom_cin_result" />

        <TextView
            android:id="@+id/number_cin_result"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="6dp"
            android:text="XY  XYYYYYY"
            app:layout_constraintEnd_toStartOf="@+id/guideline10"
            app:layout_constraintStart_toStartOf="@+id/guideline11"
            app:layout_constraintTop_toBottomOf="@+id/date_cin_result" />

        <TextView
            android:id="@+id/adresse_cin_result"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="6dp"
            android:text=" XY XY XY YXYXYXYX YXXXXXXY XYX YX YXY XY XY XY"
            app:layout_constraintEnd_toStartOf="@+id/guideline10"
            app:layout_constraintStart_toStartOf="@+id/guideline11"
            app:layout_constraintTop_toBottomOf="@+id/number_cin_result" />

        <TextView
            android:id="@+id/sexe_cin_result"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="XY"
            android:layout_marginEnd="6dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline10"
            app:layout_constraintStart_toStartOf="@+id/guideline11"
            app:layout_constraintTop_toBottomOf="@+id/adresse_cin_result" />

        <ImageView
            android:id="@+id/prenom_cin_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/check"
            app:layout_constraintStart_toStartOf="@+id/guideline10"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/nom_cin_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/warning"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="@+id/guideline10"
            app:layout_constraintTop_toBottomOf="@+id/prenom_cin_result" />

        <ImageView
            android:id="@+id/date_cin_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/check"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="@+id/guideline10"
            app:layout_constraintTop_toBottomOf="@+id/nom_cin_result" />


        <ImageView
            android:id="@+id/number_cin_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/check"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="@+id/guideline10"
            app:layout_constraintTop_toBottomOf="@+id/date_cin_result" />


        <ImageView
            android:id="@+id/adresse_cin_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/warning"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="@+id/guideline10"
            app:layout_constraintTop_toBottomOf="@+id/number_cin_result" />


        <ImageView
            android:id="@+id/sexe_cin_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:src="@drawable/check"
            android:layout_marginTop="10dp"
            app:layout_constraintStart_toStartOf="@+id/guideline10"
            app:layout_constraintTop_toBottomOf="@+id/adresse_cin_result" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_sejour_result"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/border_identity_white"
        android:elevation="10dp"
        android:padding="4dp"
        android:visibility="gone"
        app:layout_constraintEnd_toStartOf="@+id/guideline9"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toBottomOf="@+id/account_creation_txt"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/prenom_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Prénom"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/nom_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Nom"
            app:layout_constraintTop_toBottomOf="@+id/prenom_sejour_label" />

        <TextView
            android:id="@+id/date_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Date de naissance"
            app:layout_constraintTop_toBottomOf="@+id/nom_sejour_label" />

        <TextView
            android:id="@+id/number_cin_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="CIN "
            app:layout_constraintTop_toBottomOf="@+id/date_sejour_label" />

        <TextView
            android:id="@+id/adresse_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Adresse "
            app:layout_constraintTop_toBottomOf="@+id/number_cin_sejour_label" />

        <TextView
            android:id="@+id/sexe_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Sexe "
            app:layout_constraintTop_toBottomOf="@+id/adresse_sejour_label" />

        <TextView
            android:id="@+id/nationalite_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Nationalité "
            app:layout_constraintTop_toBottomOf="@+id/sexe_sejour_label" />

        <TextView
            android:id="@+id/validite_sejour_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Fin de validité "
            app:layout_constraintTop_toBottomOf="@+id/nationalite_sejour_label" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_passport_result"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/border_identity_white"
        android:elevation="10dp"
        android:padding="4dp"
        android:visibility="gone"
        app:layout_constraintEnd_toStartOf="@+id/guideline9"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toBottomOf="@+id/account_creation_txt"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/prenom_passport_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Prénom"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/nom_passport_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Nom"
            app:layout_constraintTop_toBottomOf="@+id/prenom_passport_label" />

        <TextView
            android:id="@+id/date_passport_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Date de naissance"
            app:layout_constraintTop_toBottomOf="@+id/nom_passport_label" />

        <TextView
            android:id="@+id/number_cin_passport_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="CIN "
            app:layout_constraintTop_toBottomOf="@+id/date_passport_label" />

        <TextView
            android:id="@+id/sexe_passport_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Sexe "
            app:layout_constraintTop_toBottomOf="@+id/number_cin_passport_label" />

        <TextView
            android:id="@+id/nationalite_passport_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Nationalité "
            app:layout_constraintTop_toBottomOf="@+id/sexe_passport_label" />

        <TextView
            android:id="@+id/passport_number_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Passport numéro"
            app:layout_constraintTop_toBottomOf="@+id/nationalite_passport_label" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/container_sim_result"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/border_identity_white"
        android:elevation="10dp"
        android:padding="4dp"
        android:visibility="gone"
        app:layout_constraintEnd_toStartOf="@+id/guideline9"
        app:layout_constraintStart_toStartOf="@+id/guideline8"
        app:layout_constraintTop_toBottomOf="@+id/account_creation_txt"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/prenom_sim_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Prénom"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/nom_sim_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Nom"
            app:layout_constraintTop_toBottomOf="@+id/prenom_sim_label" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <!-- Part on the bottom -->
    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="110dp"
        android:background="@drawable/gray_line"
        app:layout_constraintBottom_toBottomOf="parent" />


    <TextView
        android:id="@+id/tv_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text=""
        android:textSize="20dp"
        android:visibility="gone"
        app:layout_constraintTop_toBottomOf="@+id/account_creation_txt"
        tools:layout_editor_absoluteX="35dp" />

    <TextView
        android:id="@+id/back_Btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginHorizontal="40dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/bg_button_confim"
        android:padding="14dp"
        android:text="Retourner à la page du Scan"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFF"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="80dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.8" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />




</androidx.constraintlayout.widget.ConstraintLayout>